{
  "services": {
    "dropbear": {
      "install":"apt-get update; apt-get install -o Dpkg::Options::=--force-confdef -o Dpkg::Options::=--force-confold --no-install-recommends --no-install-suggests -y dropbear",
      "enable": "echo \"$AIRSTACK_USER_NAME:$AIRSTACK_USER_PASSWORD\" | chpasswd",
      "disable": "",
      "run": "exec chpst -u root /usr/sbin/dropbear -F -E -R",
      "check": "",
      "finish": "",
      "log": "exec chpst -u root logger -t $SERVICE_NAME -p local0.notice",
      "tags": ["ssh"],
      "input": [{"location": "", "port": "22", "protocol": "tcp", "tags": [ "ssh" ]}],
      "output": []      
    }
   }
}