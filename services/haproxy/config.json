{
  "services": {
    "haproxy": {
      "enable": "",
      "run": "exec /usr/sbin/haproxy -db -f /etc/haproxy/haproxy.cfg -p /var/run/haproxy.pid -sf $(cat /var/run/haproxy.pid); sleep 1; done",
      "check": "",
      "finish": "",
      "log": "exec chpst -u root logger -t $SERVICE_NAME -p local0.notice"
    }
   }
}