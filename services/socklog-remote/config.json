{
  "services": {
    "socklog-remote": {
      "install": "",
      "state": "up",
      "enable": ". /package/airstack/socklog-remote/enable",
      "disable": "",
      "run": "sv check socklog-unix/log >/dev/null || exit 1; ",
      "check": "",
      "finish": "rm -rf /var/log/socklog/main/toremote/; pidof svlogd && kill -HUP $(pidof svlogd) || return 0",
      "log": "",
      "tags": ["logger", "remote"],
      "input": [],
      "output": [
        {
          "source:":"any",
          "destination": "192.168.59.3",
          "port": "514",
          "protocol": "tcp",
          "tags": [ "syslog", "tcp" ]
        }
      ]
    }
  }
}
